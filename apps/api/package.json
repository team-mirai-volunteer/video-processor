{
  "name": "@video-processor/api",
  "version": "0.1.0",
  "private": true,
  "description": "Cloud Run backend API for video processor",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/infrastructure/database/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/infrastructure/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@video-processor/shared": "workspace:*",
    "ai": "^4.1.0",
    "@ai-sdk/google": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^144.0.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.10.7",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
